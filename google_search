from googlesearch import search
import pandas as pd

file = pd.read_csv("file.csv")
kw = file['keyword']

finaldf = pd.DataFrame(columns=('Keyword', 'links'))
row = 0
for j in kw:
    term = "site:dir.indiamart.com " + j
    finaldf.set_value(row, 'Keyword', j)
    print(term)
    for i in search(term, tld="com", num=20, stop=2, pause=60):
        finaldf.set_value(row, 'links', i)
        row = row + 1
    row = row + 1
    finaldf.to_csv("D:/finalOutput2.csv")

